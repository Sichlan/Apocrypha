                    <Path Fill="{TemplateBinding Foreground}" Data="M3 5v9h9V5H3zm8 8H4V6h7v7z"/>
                    <Path Fill="{TemplateBinding Foreground}" Data="M5 5h1V4h7v7h-1v1h2V3H5v2z"/>
