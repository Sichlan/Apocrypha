    <ControlTemplate x:Key="IconRestoreWindow" TargetType="{x:Type ContentControl}">
        <Viewbox Stretch="Fill" Height="{TemplateBinding Height}" Width="{TemplateBinding Width}">
            <Canvas Width="16" Height="16">
